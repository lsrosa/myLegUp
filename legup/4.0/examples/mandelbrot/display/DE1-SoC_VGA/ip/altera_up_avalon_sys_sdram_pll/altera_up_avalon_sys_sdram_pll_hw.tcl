# | Composition function
# | 
proc compose {} {
	set refclk				[ get_parameter_value "refclk" ]
	set outclk				[ get_parameter_value "outclk" ]
	set board				[ get_parameter_value "board" ]
	set device_family		[ get_parameter_value "device_family" ]


	# +-----------------------------------
	# | add instances
	# |
	if {  [ string match -nocase "Cyclone IV*" $device_family ]  ||
			[ string match -nocase "Cyclone III*" $device_family ] ||
			[ string match -nocase "Arria II*" $device_family ] ||
			[ string match -nocase "Stratix III*" $device_family ] ||
			[ string match -nocase "Stratix IV*" $device_family ]} {
		add_instance sys_pll altera_up_altpll
		set_instance_property sys_pll SUPPRESS_ALL_INFO_MESSAGES true
		set_instance_property sys_pll SUPPRESS_ALL_WARNINGS true

		set_instance_parameter_value sys_pll type "System/SDRAM"
		
	} else {
		add_instance sys_pll altera_pll
		set_instance_property sys_pll SUPPRESS_ALL_INFO_MESSAGES true
		set_instance_property sys_pll SUPPRESS_ALL_WARNINGS true

		set_instance_parameter_value sys_pll gui_reference_clock_frequency $refclk
		set_instance_parameter_value sys_pll gui_number_of_clocks 2
		set_instance_parameter_value sys_pll gui_output_clock_frequency0 $outclk
		set_instance_parameter_value sys_pll gui_output_clock_frequency1 $outclk
		if { $board == "DE1-SoC" } {
			set_instance_parameter_value sys_pll gui_ps_units1 "degrees"
			set_instance_parameter_value sys_pll gui_phase_shift_deg1  "-90.0"
		} else {
			set_instance_parameter_value sys_pll gui_ps_units1 "ps"
			set_instance_parameter_value sys_pll gui_phase_shift_deg1  "-3000"
		}
	}

	add_instance reset_from_locked altera_up_avalon_reset_from_locked_signal
	# | 
	# +-----------------------------------


	# +-----------------------------------
	# | add connection points
	# | 
	add_interface ref_clk clock end
	add_interface ref_reset reset end
	add_interface sys_clk clock start
	if { $board != "None" } {
		add_interface sdram_clk clock start
	}
	add_interface reset_source reset start
	
	set_interface_property ref_clk EXPORT_OF "sys_pll.refclk"
	set_interface_property ref_reset EXPORT_OF "sys_pll.reset"
	set_interface_property sys_clk EXPORT_OF "sys_pll.outclk0"
	if { $board != "None" } {
		set_interface_property sdram_clk EXPORT_OF "sys_pll.outclk1"
	}
	set_interface_property reset_source EXPORT_OF "reset_from_locked.reset_source"

	add_connection "sys_pll.locked" "reset_from_locked.locked" conduit
	# | 
	# +-----------------------------------
}
