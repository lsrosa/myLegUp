proc validate {} {
	set gui_refclk				[ get_parameter_value "gui_refclk" ]
	set gui_outclk				[ get_parameter_value "gui_outclk" ]
	set CIII_boards			[ get_parameter_value "CIII_boards" ]
	set CIV_boards				[ get_parameter_value "CIV_boards" ]
	set CV_boards				[ get_parameter_value "CV_boards" ]
#	set SIII_boards			[ get_parameter_value "SIII_boards" ]
#	set SIV_boards				[ get_parameter_value "SIV_boards" ]
#	set SV_boards				[ get_parameter_value "SV_boards" ]
	set other_boards			[ get_parameter_value "other_boards" ]
	set board					[ get_parameter_value "board" ]
	set device_family			[ get_parameter_value "device_family" ]
	
	set_parameter_property CIII_boards VISIBLE false
	set_parameter_property CIV_boards VISIBLE false
	set_parameter_property CV_boards VISIBLE false
#	set_parameter_property SIII_boards VISIBLE false
#	set_parameter_property SIV_boards VISIBLE false
#	set_parameter_property SV_boards VISIBLE false
	set_parameter_property other_boards VISIBLE false

	if { [ string match -nocase "Cyclone III*" $device_family ] } {
		set_parameter_property CIII_boards VISIBLE true
		set_parameter_value board $CIII_boards
	} elseif { [ string match -nocase "Cyclone IV*" $device_family ] } {
		set_parameter_property CIV_boards VISIBLE true
		set_parameter_value board $CIV_boards
	} elseif { [ string match -nocase "Cyclone V*" $device_family ] } {
		set_parameter_property CV_boards VISIBLE true
		set_parameter_value board $CV_boards
#	} elseif { [ string match -nocase "Stratix III*" $device_family ] } {
#		set_parameter_property SIII_boards VISIBLE true
#		set_parameter_value board $SIII_boards
#	} elseif { [ string match -nocase "Stratix IV*" $device_family ] } {
#		set_parameter_property SIV_boards VISIBLE true
#		set_parameter_value board $SIV_boards
#	} elseif { [ string match -nocase "Stratix V*" $device_family ] } {
#		set_parameter_property SV_boards VISIBLE true
#		set_parameter_value board $SV_boards
	} else {
		set_parameter_property other_boards VISIBLE true
		set_parameter_value board $other_boards
	}
	
	set_parameter_property gui_refclk VISIBLE false
	set_parameter_property refclk VISIBLE false
	set_parameter_property gui_outclk VISIBLE false
	set_parameter_property outclk VISIBLE false

	if {  [ string match -nocase "Cyclone IV*" $device_family ]  || 
			[ string match -nocase "Cyclone III*" $device_family ] ||
			[ string match -nocase "Arria II*" $device_family ] ||
			[ string match -nocase "Stratix III*" $device_family ] ||
			[ string match -nocase "Stratix IV*" $device_family ]} {
		set_parameter_property refclk VISIBLE true
		set_parameter_property outclk VISIBLE true
		set_parameter_value refclk 50.0
		set_parameter_value outclk 50.0
		
	} else {
		set_parameter_property gui_refclk VISIBLE true
		set_parameter_property gui_outclk VISIBLE true
		set_parameter_value refclk $gui_refclk
		set_parameter_value outclk $gui_outclk
	}
}
